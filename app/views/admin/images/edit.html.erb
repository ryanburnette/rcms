<div class="panel panel-default edit-image">
  <div class="panel-heading"><h2>Edit Image</h2></div>
  <div class="panel-body">
    <div class="form">
      <%= form_for(:image, :url => {:action => 'update'}) do |f| %>
      <%= render(:partial => "form", :locals => {:f => f}) %>
      <div class="form-buttons">
        <%= button_to 'Update', edit_admin_image_path(@image), method: :patch, class: 'btn btn-primary' %>
        <%= link_to 'Back', admin_images_path, {:class => "btn btn-default"} %>
      </div>
      <% end %>
    </div>
  </div>
</div>
