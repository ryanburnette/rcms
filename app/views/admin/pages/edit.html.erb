<div class="panel panel-default">
  <div class="panel-heading"><h2>Edit Page</h2></div>
  <div class="panel-body">
    <%= form_for(:page, :url => {:action => 'update', :id => @page.id}) do |f| %>
    <div class="form">
      <%= render(:partial => "form", :locals => {:f => f}) %>
      <div class="form-buttons">
        <%= button_to 'Update', edit_admin_page_path(@page.id), method: :patch, class: 'btn btn-primary' %>
        <%= link_to 'Back', admin_pages_path, {:class => "btn btn-default"} %>
      </div>
    </div>
    <% end %>
  </div>
</div>
